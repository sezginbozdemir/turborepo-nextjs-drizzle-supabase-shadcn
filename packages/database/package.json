{
  "name": "@repo/database",
  "version": "0.0.0",
  "private": true,
  "exports": {
    "./drizzle/*": "./drizzle/*.ts",
    "./supabase/*": "./supabase/*.ts"
  },
  "scripts": {
    "dev": "tsc --noEmit -w --preserveWatchOutput",
    "db:push": "drizzle-kit push --config ./drizzle/drizzle.config.ts",
    "db:up": "drizzle-kit up --config ./drizzle/drizzle.config.ts",
    "db:generate": "drizzle-kit generate --config ./drizzle/drizzle.config.ts",
    "db:pull": "drizzle-kit introspect --config ./drizzle/drizzle.config.ts",
    "db:migrate": "drizzle-kit migrate --config ./drizzle/drizzle.config.ts",
    "db:studio": "drizzle-kit studio --config ./drizzle/drizzle.config.ts",
    "sb:types": "tsx ./supabase/generate-types.ts"
  },
  "devDependencies": {
    "@types/node": "^22.15.3",
    "drizzle-kit": "^0.31.7",
    "tsx": "^4.20.3",
    "typescript": "5.8.2"
  },
  "dependencies": {
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.52.1",
    "@types/pg": "^8.15.5",
    "drizzle-orm": "^0.44.4",
    "pg": "^8.16.3"
  }
}
